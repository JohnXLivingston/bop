{
  "name": "bop",
  "version": "0.0.1",
  "private": true,
  "main": "dist/server.js",
  "description": "Bop Open Planning",
  "scripts": {
    "clean": "rimraf dist/*",
    "lint": "npm-run-all -s config:lint sass:lint tsc:lint dependencies:lint",
    "config:lint": "yamllint config/**/*.yaml",
    "sass:lint": "sass-lint --verbose",
    "tsc:lint": "eslint --ext .ts ./src ./src/public ./tests/",
    "build": "NODE_ENV=production npm run common:build",
    "dev:build": "NODE_ENV=development npm run common:build",
    "common:build": "npm-run-all -s clean -p public:build html:build tsc:build i18n:build",
    "public:build": "webpack --config webpack.config.js",
    "html:build": "scripty",
    "tsc:build": "npx tsc",
    "common:start": "node .",
    "dev:start": "NODE_ENV=development nodemon --watch dist -e js,html,css,json,png --delay 2 --exec npm run common:start",
    "dev": "NODE_ENV=development npm-run-all -s clean dev:build -p public:watch tsc:watch html:watch dev:start i18n:watch",
    "public:watch": "npm run public:build -- --watch",
    "tsc:watch": "npx tsc --watch",
    "html:watch": "watch 'npm run html:build' src/views/ src/shared/templates/",
    "start": "NODE_ENV=production npm-run-all -s build common:start",
    "test": "scripty",
    "test:nolint": "TEST_NOLINTING=1 npm run test",
    "i18n:extract": "npx i18next -c .i18next-parser.config.js",
    "i18n:build": "cp -r src/locales/ dist/i18n/",
    "i18n:watch": "watch 'npm run i18n:build' src/locales/",
    "database:sync:launch": "NODE_ENV=production node ./dist/scripts/database-sync.js",
    "dev:database:sync": "NODE_ENV=development npm-run-all -s clean dev:build dev:database:sync:launch",
    "dev:database:sync:launch": "NODE_ENV=development node ./dist/scripts/database-sync.js",
    "dependencies:graph": "scripty",
    "dependencies:lint": "depcruise --output-type err-long --config -- src",
    "repl:launch": "node --experimental-repl-await dist/scripts/repl.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/JohnXLivingston/bop.git"
  },
  "keywords": [
    "Applicative",
    "Framework"
  ],
  "author": "Jonathan Doll√©",
  "license": "AGPL-3.0",
  "bugs": {
    "url": "https://github.com/JohnXLivingston/bop/issues"
  },
  "homepage": "https://github.com/JohnXLivingston/bop#readme",
  "dependencies": {
    "@babel/polyfill": "^7.12.1",
    "bcrypt": "^5.0.0",
    "body-parser": "^1.19.0",
    "config": "^3.3.3",
    "connect-redis": "^5.0.0",
    "cookie-parser": "^1.4.5",
    "core-js": "^3.8.3",

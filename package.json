    "reflect-metadata": "^0.1.13",
    "regenerator-runtime": "^0.13.7",
    "require-json5": "^1.1.0",
    "sequelize": "^5.22.3",
    "sequelize-typescript": "^1.1.0",
    "socket.io": "^2.4.1",
    "socket.io-emitter": "^3.2.0",
    "socket.io-redis": "^5.2.0",
    "tsconfig-paths": "^3.9.0",
    "uid2": "0.0.3",
    "uuid": "^8.3.2",
    "winston": "^3.3.3",
    "winston-syslog": "^2.4.4"
